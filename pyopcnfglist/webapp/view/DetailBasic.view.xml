			<Text text="{DetailModel>/SourceReferenceID}" tooltip="{i18n>SourceReferenceID}"/>
			<Label text="{i18n>CreatedBy}" tooltip="{i18n>CreatedBy}"/>
			<Text text="{DetailModel>/CreatedBy}" tooltip="{i18n>CreatedBy}"/>
			<Label text="{i18n>CreatedOn}" tooltip="{i18n>CreatedOn}"/>
			<Text text="{path:'DetailModel>/CreatedOn', formatter: 'com.arteriatech.ss.utils.js.Common.getFormattedDate'}"
